{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },

  "extensions": {
    "openApi": {
      "enabled": true,
      "forceHttps": false,
      "swaggerUi": {
        "authLevel": "anonymous"
      }
    }
  }
}